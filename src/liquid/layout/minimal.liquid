
<!doctype html>
<html class="no-js supports-no-cookies" lang="{{ shop.locale }}">
  <head>
    {%- render 'template-head' -%}
    {{ content_for_header }}
  </head>

  <body class="template-{{ template.name | handle }} layout--minimal">
    {%- render 'icon-store' -%}

    <div id="app" v-cloak>
      <main role="main" id="MainContent">
        {{ content_for_layout }}
      </main>

      <toast></toast>
    </div>

    {%- render 'template-scripts' -%}
  </body>
</html>
